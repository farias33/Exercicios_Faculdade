#include <stdio.h>

int main()
{
    int a,b,c,m;
    printf("Digite a idade dos filhos:\n");
    scanf("%d%d%d", &m, &a, &b);
    
    c = m - a - b;
    
    if(a>b && a>c){
        printf("%d\n", a);
        
    }
    else if(b>a && b>c) {
        printf("%d\n", b);
    }
    else{
        printf("%d", c);
    }
    
    return 0;
}
